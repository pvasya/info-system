{
  "id": null,
  "uid": "postgres-overview",
  "title": "Postgres & App Overview",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "graph",
      "title": "DB Connections",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "pg_stat_activity_count{job=\"postgres\"}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "type": "graph",
      "title": "Cache Hit Ratio",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "pg_stat_database_blks_hit{job=\"postgres\",datname=\"online_store\"} / (pg_stat_database_blks_hit{job=\"postgres\",datname=\"online_store\"} + pg_stat_database_blks_read{job=\"postgres\",datname=\"online_store\"})",
          "refId": "B"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "type": "stat",
      "title": "HTTP Availability",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "probe_success{job=\"blackbox\"}",
          "refId": "C"
        }
      ],
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 8}
    },
    {
      "type": "graph",
      "title": "Response Time",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "probe_duration_seconds{job=\"blackbox\"}",
          "refId": "D"
        }
      ],
      "gridPos": {"h": 6, "w": 16, "x": 8, "y": 8}
    },
    {
      "type": "stat",
      "title": "Homepage Content Length",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "probe_http_uncompressed_body_length{job=\"blackbox\",instance=\"http://app:8000/\"}",
          "refId": "E"
        }
      ],
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 14}
    }
    ,
    {
      "type": "bargauge",
      "title": "Rows per Table (Bar)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "pg_stat_user_tables_n_live_tup{job=\"postgres\",datname=\"online_store\"}",
          "refId": "F",
          "instant": true,
          "legendFormat": "{{relname}}"
        }
      ],
      "fieldConfig": {
        "defaults": {"unit": "none"},
        "overrides": []
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "showUnfilled": true,
        "reduceOptions": {"calcs": ["last"], "fields": "", "values": false}
      },
      "gridPos": {"h": 8, "w": 16, "x": 8, "y": 14}
    },
    {
      "type": "bargauge",
      "title": "Dead Tuples per Table (Bar)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "pg_stat_user_tables_n_dead_tup{job=\"postgres\",datname=\"online_store\"}",
          "refId": "G",
          "instant": true,
          "legendFormat": "{{relname}}"
        }
      ],
      "fieldConfig": {
        "defaults": {"unit": "none"},
        "overrides": []
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "showUnfilled": true,
        "reduceOptions": {"calcs": ["last"], "fields": "", "values": false}
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22}
    }
  ],
  "templating": {"list": [
    {
      "type": "query",
      "name": "table",
      "label": "Table",
      "datasource": "Prometheus",
      "query": "label_values(pg_stat_user_tables_n_live_tup{job=\"postgres\",datname=\"online_store\"}, relname)",
      "definition": "label_values(pg_stat_user_tables_n_live_tup{job=\"postgres\",datname=\"online_store\"}, relname)",
      "refresh": 2,
      "includeAll": true,
      "multi": false,
      "sort": 1,
      "current": {"text": "All", "value": "$__all"}
    }
  ]},
  "time": {"from": "now-15m", "to": "now"},
  "timepicker": {}
}
